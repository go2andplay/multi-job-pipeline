#!/usr/bin/env groovy

pipeline {

    agent any

    triggers { pollSCM('* * * * *') }


    stages {

        stage('Checkout Stage2') {
            steps {
                print "Stage2 "
            }
        }


        stage('Build Stage2') {
            steps {
                print "Stage2 "
            }
        }

        stage('Dockerize Stage2') {
            steps {
                print "Stage2 "
            }
        }

        stage('Trigger New stage') {
            steps {
                print "Trigger new stage "
            }
        }

    }
}